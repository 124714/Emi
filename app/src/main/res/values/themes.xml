    </style>
    <style name="CustomChipChoice" parent="@style/Widget.MaterialComponents.Chip.Choice">
        <item name="chipIcon">@drawable/visibility</item>
        <item name="checkedIcon">@drawable/hide</item>
    </style>



</resources>